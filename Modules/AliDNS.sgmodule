#!date = 2024-08-03 11:43:57
#!category = DNS解析
#!arguments = 阿里DNS:223.5.5.5,请求域名:*,请求类型:1,设备ID:IOS,账户ID,AccessID,Secret
#!arguments-desc = 相关参数说明查阅 阿里云DoH JSON API 文档


[Host]
{{{请求域名}}}= script:Alidns

[Script]
Alidns = type=dns,engine=jsc,script-path=https://raw.githubusercontent.com/Maasea/sgmodule/master/Script/Tools/alidns.js,argument="{"HOST":"{{{阿里DNS}}}","UID":"{{{账户ID}}}","ID":"{{{AccessID}}}","SECRET":"{{{Secret}}}","DID":"{{{设备ID}}}","TYPE":"{{{请求类型}}}"}"